{
  "expo": {
    "name": "ecommerce-app",
    "slug": "ecommerce-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/farmtoplate.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/farmtoplate.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "We need to access your camera to take pictures.",
        "NSPhotoLibraryUsageDescription": "We need access to your photo library to upload images."
      }
    },
    "android": {
      "adaptiveIcon": {
        "package": "com.teamvatte.farmtoplate",
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "jsEngine": "hermes",
      "newArchEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-asset"
    ]
  }
}
